<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>PG Insight</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="~/Content/build/js/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/build/css/separate/vendor/bootstrap-select/bootstrap-select.min.css">
    <link href="~/Content/build/css/separate/Ionicons/css/ionicons.css" rel="stylesheet" />
    <link href="~/Content/build/css/lib/select2/dist/css/select2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/build/css/lib/datatables-net/datatables.min.css">
    <link rel="stylesheet" href="~/Content/build/css/separate/vendor/datatables-net.min.css" />

    <link rel="stylesheet" href="~/Content/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css"/>

    <!-- Theme style -->
    <link href="~/Content/plugins/bootstrap-toastr/toastr.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
        folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/build/css/lib/charts-c3js/c3.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="~/Content/dist/css/skins/skin-green.css">

    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    @RenderSection("StyleSection", false)
</head>
<body class="hold-transition skin-green sidebar-mini ">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">PGInsight</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">
                    <img src="~/Content/dist/img/logo_pginsight_2018.png" style="max-width: 120px; padding-bottom: 4px">
                </span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="hidden-xs">Sign Out <i class="fa fa-sign-out"></i></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a href="/Account/LogOff" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        @Html.Partial("~/Views/Shared/_SidebarMenu.cshtml")
        <div class="content-wrapper bgpetro">
            @RenderBody()
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Time</b> :  12:00:21
            </div>
            <strong>Copyright &copy; 2018. PT.PETROKIMIA GRESIK.</strong> All rights
            reserved.
        </footer>
        <!-- jQuery 3 -->
        <!-- Slimscroll -->
        @*<script src="~/Content/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>*@
        <!-- FastClick -->

        <script src="~/Content/build/js/lib/jquery/jquery.min.js"></script>
        <script src="~/Content/build/js/lib/tether/tether.min.js"></script>
        <script src="~/Content/build/js/lib/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="~/Content/build/js/lib/jquery-slimscroll/jquery.slimscroll.min.js"></script>
        <script src="~/Scripts/jquery.validate.js"></script>
        <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
        <script src="~/Content/build/css/lib/select2/dist/js/select2.full.min.js"></script>
        <script src="~/Content/build/js/lib/bootstrap-select/bootstrap-select.min.js"></script>
        <script src="~/Content/build/js/lib/datatables-net/datatables.min.js"></script>
        <script src="~/Content/build/js/lib/chart.js/Chart.js"></script>
        @*<script src="~/Content/bower_components/chart.js/Chart.js"></script>*@
        <script src="~/Content/build/js/lib/fastclick/lib/fastclick.js"></script>
        <!-- AdminLTE App -->
        <script src="~/Content/dist/js/adminlte.min.js"></script>
        <script src="~/Content/plugins/bootstrap-toastr/toastr.js"></script>
        <script src="~/Scripts/bootbox.min.js"></script>
        <!-- AdminLTE for demo purposes -->
        <script src="~/Scripts/modalForm.js"></script>
        <script src="~/Scripts/pathseg.js"></script>
        <script src="~/Content/dist/js/demo.js"></script>
        <script src="~/Content/build/js/lib/d3/d3.min.js"></script>
        <script src="~/Content/build/js/lib/charts-c3js/c3.min.js"></script>
        <script src="~/Content/build/js/lib/charts-c3js/c3js-init.js"></script>


        <script src="~/Content/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>

        @RenderSection("scripts", false);
        @*@RenderSection("scripts", required: false)*@
        <script>
            $(document).ready(function () {
                $('.select2').select2()
                $('.datepicker').datepicker()
            });
        </script>
</body>
</html>
